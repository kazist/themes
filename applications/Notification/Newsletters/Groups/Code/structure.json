    "query": {
        "from": {
            "table": "Notification\\Newsletters\\Groups\\Code\\Tables\\Groups",
            "alias": "nng",
            "name": "#__notification_newsletters_groups"
        },
        "select": [
            "nng.id",
            "nng.group_id",
            "ng.name AS group_id_name",
            "nng.newsletter_id",
            "nn.subject AS newsletter_id_subject",
            "nng.published",
            "nng.created_by",
            "nng.date_created",
            "nng.modified_by",
            "nng.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Notification\\Groups\\Code\\Tables\\Groups",
                "alias": "ng",
                "name": "#__notification_groups",
                "on": "nng.group_id = ng.id"
            },
            {
                "table": "Notification\\Newsletters\\Code\\Tables\\Newsletters",
                "alias": "nn",
                "name": "#__notification_newsletters",
                "on": "nng.newsletter_id = nn.id"
            }
        ]
    }
